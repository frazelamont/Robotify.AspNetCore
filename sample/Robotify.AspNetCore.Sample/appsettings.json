{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Robotify": {
    "Enabled": true,
    "SitemapUrl": "https://www.example.com/sitemap.xml",
    "CrawDelay": 10,
    "DisableFileHeaderComments" : false,
    "Groups": [
      {
        "UserAgent": "*",
        "Disallow": "/"
      },
      {
        "UserAgent": "Googlebot",
        "Disallow": "/admin"
      },
      {
        "UserAgent": "AnotherBot",
        "Allow": "/search"
      }
    ] 
  } 
}
